{
  "name": "electron-ffmpeg-eq",
  "version": "0.1.0",
  "description": "Minimal Electron + FFmpeg EQ proof of concept",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:auto": "DEV_AUTO_LOAD_FILE=media/IMG_0061-30sec.MOV concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:edit": "ASR_BACKEND=elevenlabs DEV_AUTO_LOAD_FILE=media/IMG_0061-30sec.MOV vite",
    "electron:build": "vite build && electron-builder",
    "test": "vitest",
    "test:alpha": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "form-data": "^4.0.5",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "jsdom": "^27.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^4.0.16",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.test.electron-ffmpeg-eq",
    "mac": {
      "target": "dir"
    }
  }
}
